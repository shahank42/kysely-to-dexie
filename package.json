  "homepage": "https://github.com/shahank42/kysely-to-dexie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahank42/kysely-to-dexie.git"
  },
  "bugs": "https://github.com/shahank42/kysely-to-dexie/issues",
  "author": "Shounak Ghosh <shounakghosh42@gmail.com>",
  "devDependencies": {
    "@types/bun": "^1.1.10",
    "bun-plugin-dts": "^0.3.0",
    "fake-indexeddb": "^6.0.1"
  },
  "dependencies": {
    "dexie": "^4.0.11",
    "kysely": "^0.28.2"
